// Test for loop constructs
print "=== For Loop Tests ===";

// Basic for loop
print "Basic for loop:";
for (var i = 0; i < 5; i = i + 1) {
  print i;
}

// For loop with different increment
print "For loop with step 2:";
for (var j = 0; j < 10; j = j + 2) {
  print j;
}

// For loop with decrement
print "For loop decrementing:";
for (var k = 5; k > 0; k = k - 1) {
  print k;
}

// For loop with no initialization
print "For loop with pre-initialized variable:";
var m = 3;
for (; m < 6; m = m + 1) {
  print m;
}

// For loop with no condition (infinite loop with break)
print "For loop with break:";
var n = 0;
for (;;) {
  if (n >= 3) {
    break;
  }
  print n;
  n = n + 1;
}

// For loop with no increment
print "For loop with manual increment:";
for (var p = 0; p < 3;) {
  print p;
  p = p + 1;
}

// Nested for loops
print "Nested for loops:";
for (var outer = 1; outer <= 2; outer = outer + 1) {
  for (var inner = 1; inner <= 2; inner = inner + 1) {
    print "(" + outer + "," + inner + ")";
  }
}

// For loop with complex expressions
print "For loop with complex expressions:";
for (var x = 1; x * x <= 16; x = x + 1) {
  print x + " squared is " + (x * x);
}

// For loop variable scoping
print "For loop variable scoping:";
var outside = "outside";
for (var outside = 0; outside < 2; outside = outside + 1) {
  print "inside: " + outside;
}
print "after loop: " + outside;

print "For loop tests completed";
