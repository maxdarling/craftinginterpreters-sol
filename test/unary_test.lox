// Test unary operators (-, !)
print "=== Unary Operators Tests ===";

// Negation operator (-)
print -5; // -5
print -(-5); // 5
print -0; // -0 (which is 0)
print -1.5; // -1.5
print -(-1.5); // 1.5

// Negation with variables
var x = 10;
print -x; // -10
print x; // 10 (x should be unchanged)

// Negation with expressions
print -(1 + 2); // -3
print -(5 * 2); // -10
print -(10 / 2); // -5.0

// Logical NOT operator (!)
print !true; // false
print !false; // true
print !!true; // true
print !!false; // false

// NOT with truthy/falsy values (only nil and false are falsy)
print !nil; // true (nil is falsy)
print !0; // false (0 is truthy)
print !""; // false (empty string is truthy)
print !1; // false (1 is truthy)
print !"hello"; // false (non-empty string is truthy)

// NOT with expressions
print !(1 == 1); // false
print !(1 != 1); // true
print !(5 > 3); // false
print !(5 < 3); // true

// Complex unary expressions
print !!(1 + 1); // true (double negation of truthy value)
print !!nil; // false (double negation of falsy value)
print -(-(-5)); // -5 (triple negation)

// Mixed unary and binary operators
print -5 + 3; // -2
print !true and false; // false
print !false or true; // true
print -(2 * 3) + 1; // -5

// Unary with function calls
fun getNumber() {
  return 42;
}

print -getNumber(); // -42
print !getNumber(); // false (42 is truthy)

print "Unary tests completed";
